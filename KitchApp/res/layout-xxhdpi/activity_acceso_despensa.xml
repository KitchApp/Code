	    <Button
	        android:id="@+id/buttonDrinks"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignLeft="@+id/buttonFruits"
	        android:layout_below="@+id/buttonFruits"
	        android:layout_marginTop="48dp"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/bebidas"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonBread"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignLeft="@+id/buttonDairy"
	        android:layout_alignTop="@+id/buttonDrinks"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/panBolleria"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonMeat"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignBaseline="@+id/buttonFish"
	        android:layout_alignBottom="@+id/buttonFish"
	        android:layout_alignLeft="@+id/buttonBread"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/carnes"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonFish"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignLeft="@+id/buttonDrinks"
	        android:layout_centerVertical="true"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/pescados"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonPasta"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_above="@+id/buttonFrozen"
	        android:layout_alignLeft="@+id/buttonFish"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/pastasArroces"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonOthers"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignBaseline="@+id/buttonFrozen"
	        android:layout_alignBottom="@+id/buttonFrozen"
	        android:layout_alignLeft="@+id/buttonPasta"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/varios"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonSauces"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignLeft="@+id/buttonMeat"
	        android:layout_below="@+id/buttonMeat"
	        android:layout_marginTop="39dp"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/salsasCondimentos"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonFrozen"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignLeft="@+id/buttonSauces"
	        android:layout_below="@+id/buttonSauces"
	        android:layout_marginTop="44dp"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/congelados"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/button_lens"
	        android:layout_width="100dp"
	        android:layout_height="60dp"
	        android:layout_alignParentBottom="true"
	        android:layout_marginBottom="27dp"
	        android:layout_marginLeft="40dp"
	        android:layout_toRightOf="@+id/textView1"
	        android:background="@drawable/lupa_sobre_un_robot_androide" />

	    <TextView
	        android:id="@+id/textView1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerHorizontal="true"
	        android:text="@string/despensa"
	        android:textColor="@color/navy"
	        android:textSize="50sp" />

	    <Button
	        android:id="@+id/buttonFruits"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_alignParentRight="true"
	        android:layout_below="@+id/textView1"
	        android:layout_marginRight="16dp"
	        android:layout_marginTop="50dp"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/frutasVerduras"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	    <Button
	        android:id="@+id/buttonDairy"
	        android:layout_width="155dp"
	        android:layout_height="85dp"
	        android:layout_above="@+id/buttonDrinks"
	        android:layout_marginRight="18dp"
	        android:layout_toLeftOf="@+id/button_lens"
	        android:background="@drawable/boton_redondo_color_celeste"
	        android:text="@string/lacteos"
	        android:textColor="@color/black"
	        android:textSize="25dp" />

	</RelativeLayout>

</ScrollView>