<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
    	android:text="@string/despensa"
    	android:textColor="@color/navy"
    	android:textSize="30sp" >

    </TextView>
    
    <Button
   	    android:id="@+id/button1"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentLeft="true"
   	    android:layout_below="@+id/textView1"
   	    android:layout_marginLeft="33dp"
   	    android:layout_marginTop="10dp"
   	    android:background="@color/aqua"
   	    android:text="@string/lacteos"
   	    android:textColor="@color/black" />
   	
   	<Button
   	    android:id="@+id/button2"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/textView1"
   	    android:layout_marginRight="33dp"
   	    android:layout_marginTop="10dp"
   	    android:background="@color/aqua"
   	    android:text="@string/frutasVerduras"
   	    android:textColor="@color/black" />
   	
   	<Button
   	    android:id="@+id/button3"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentLeft="true"
   	    android:layout_below="@+id/button1"
   	    android:layout_marginLeft="33dp"
   	    android:layout_marginTop="10dp"
   	    android:background="@color/aqua"
   	    android:text="@string/panBolleria"
   	    android:textColor="@color/black" />

   	<Button
       	android:id="@+id/button4"
       	android:layout_width="107dp"
       	android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/button2"
   	    android:layout_marginRight="33dp"
   	    android:layout_marginTop="10dp"
       	android:background="@color/aqua"
       	android:text="@string/bebidas"
       	android:textColor="@color/black" />
   	
   	<Button
   	    android:id="@+id/button5"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentLeft="true"
   	    android:layout_below="@+id/button3"
   	    android:layout_marginLeft="33dp"
   	    android:layout_marginTop="10dp"
   	    android:background="@color/aqua"
   	    android:text="@string/carnes"
   	    android:textColor="@color/black" />

   	<Button
       	android:id="@+id/button6"
       	android:layout_width="107dp"
       	android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/button4"
   	    android:layout_marginRight="33dp"
   	    android:layout_marginTop="10dp"
       	android:background="@color/aqua"
       	android:text="@string/pescados"
       	android:textColor="@color/black" />
   	
   	<Button
   	    android:id="@+id/button7"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentLeft="true"
   	    android:layout_below="@+id/button5"
   	    android:layout_marginLeft="33dp"
   	    android:layout_marginTop="10dp"
   	    android:background="@color/aqua"
   	    android:text="@string/salsasCondimentos"
   	    android:textColor="@color/black" />

   	<Button
       	android:id="@+id/button8"
       	android:layout_width="107dp"
       	android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/button6"
   	    android:layout_marginRight="33dp"
   	    android:layout_marginTop="10dp"
       	android:background="@color/aqua"
       	android:text="@string/pastasArroces"
       	android:textColor="@color/black" />
   	
   	<Button
   	    android:id="@+id/button9"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentLeft="true"
   	    android:layout_below="@+id/button7"
   	    android:layout_marginLeft="33dp"
   	    android:layout_marginTop="10dp"
   	    android:background="@color/aqua"
   	    android:text="@string/congelados"
   	    android:textColor="@color/black" />

   	<Button
       	android:id="@+id/button10"
       	android:layout_width="107dp"
       	android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/button8"
   	    android:layout_marginRight="33dp"
   	    android:layout_marginTop="10dp"
       	android:background="@color/aqua"
       	android:text="@string/varios"
       	android:textColor="@color/black" />
   	
   	<Button
   	    android:id="@+id/button11"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentLeft="true"
   	    android:layout_below="@+id/button9"
   	    android:layout_marginLeft="33dp"
   	    android:layout_marginTop="15dp"
   	    android:background="@color/navy"
   	    android:text="@string/anadirProducto"
   	    android:textColor="@color/white" />
   	
   	<Button
   	    android:id="@+id/button12"
   	    android:layout_width="107dp"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/button10"
   	    android:layout_marginRight="33dp"
   	    android:layout_marginTop="15dp"
   	    android:background="@color/navy"
   	    android:text="@string/buscarProducto"
   	    android:textColor="@color/white" />
   	
</RelativeLayout>